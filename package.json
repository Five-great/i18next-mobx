{
  "name": "i18next-mobx",
  "version": "0.0.0-alpha.1",
  "description": "A management scheme for i18nxet mobx",
  "main": "dist/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "ts-node ./src/index.ts -P tsconfig.json --no-cache",
    "build:esm": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.build.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build": "tsc -P tsconfig.json",
    "build:all": "npm run e2e && npm run build && npm run build:esm && npm run build:cjs",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "node ./test/test.js",
    "e2e": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Five-great/i18next-mobx.git"
  },
  "keywords": [
    "i18n",
    "i18next",
    "mobx"
  ],
  "author": "Five",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Five-great/i18next-mobx/issues"
  },
  "homepage": "https://github.com/Five-great/i18next-mobx#readme",
  "devDependencies": {
    "@jest/types": "^29.3.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "husky": "^8.0.2",
    "i18next-browser-languagedetector": "^7.0.1",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "react": "16.8.0",
    "react-i18next": "^12.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "preerDependencies": {
    "i18next": ">=22.0.6",
    "mobx": ">=5.x.x"
  },
  "dependencies": {
    "i18next": "^22.0.6",
    "mobx": "5.x.x"
  }
}